{
    "extends": "tslint:latest",
    "defaultSeverity": "error",
    "rules": {
        "indent": [true, "tabs"],
        "max-classes-per-file": false,
        "member-access": false,
        "no-submodule-imports": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "prefer-for-of": false,
        "quotemark": [true, "single"],
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore"
            ]
        }
    }
}
